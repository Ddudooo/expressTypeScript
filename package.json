        "request-promise-native": "^1.0.8",
        "sequelize": "^5.21.2",
        "sequelize-typescript": "^1.0.0",
        "socket.io": "^2.3.0",
        "socket.io-client": "^2.3.0",
        "socket.io-stream": "^0.9.1",
        "validator": "^12.1.0",
        "winston": "^3.2.1",
        "winston-daily-rotate-file": "^4.3.0"
    },
    "description": ""
}
