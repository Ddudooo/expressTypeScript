{
    "name": "expressTypeScript",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "test": "nyc mocha -r ts-node/register src/test/**/*.test.ts",
        "build": "tsc",
        "clean": "rimraf dist/",
        "copy": "copyfiles -u 1 \"src/**/!(*.ts)\" dist",
        "dev": "nodemon src/bin/www.ts",
        "start": "npm run clean && npm run build && npm run copy && node dist/app.js"
    },
    "nyc": {
        "extension": [
            ".ts",
            ".tsx"
        ],
        "include": [
            "src/**/*.ts"
        ],
        "exclude": [
            "src/bin/**/*",
            "src/config/**/*",
            "src/media/**/*",
            "src/upload/**/*",
            "src/log/**/*",
            "src/test/**/*"
        ],
        "reporter": [
            "html"
        ],
        "all": true
    },
    "nodemonConfig": {
        "watch": "src/**/*",
        "ignore": [
            "dist/*",
            "src/media/**/*",
            "src/upload/**/*",
            "src/log/**/*",
            "src/test/**/*"
        ],
        "delay": "2500"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/body-parser": "^1.17.1",
        "@types/chai": "^4.2.6",
        "@types/cheerio": "^0.22.15",
        "@types/cookie-parser": "^1.4.2",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.2",
        "@types/express-session": "^1.15.16",
        "@types/fluent-ffmpeg": "^2.1.11",
        "@types/http-errors": "^1.6.3",
        "@types/jsonwebtoken": "^8.3.5",
        "@types/mocha": "^5.2.7",
        "@types/morgan": "^1.7.37",
        "@types/multer": "^1.3.10",
        "@types/node": "^12.12.14",
        "@types/node-rsa": "^1.0.0",
        "@types/sequelize": "^4.28.8",
        "@types/sinon": "^7.5.1",
        "@types/socket.io": "^2.1.4",
        "@types/supertest": "^2.0.8",
        "@types/validator": "^12.0.1",
        "@typescript-eslint/eslint-plugin": "^2.10.0",
        "@typescript-eslint/parser": "^2.10.0",
        "chai": "^4.2.0",
        "cheerio": "^1.0.0-rc.3",
        "copyfiles": "^2.1.1",
        "eslint": "^6.7.2",
        "express": "^4.17.1",
        "jsonwebtoken": "^8.5.1",
        "mocha": "^6.2.2",
        "node-mocks-http": "^1.8.0",
        "nodemon": "^2.0.1",
        "nyc": "^14.1.1",
        "sequelize-cli": "^5.5.1",
        "sinon": "^7.5.0",
        "supertest": "^4.0.2",
        "ts-node": "^8.5.4",
        "typescript": "^3.7.2"
    },
    "dependencies": {
        "@google-cloud/speech": "^3.4.0",
        "@google-cloud/text-to-speech": "^2.0.2",
        "@google-cloud/translate": "^5.1.0",
        "@types/request-promise-native": "^1.0.17",
        "body-parser": "^1.19.0",
        "cookie-parser": "^1.4.4",
        "dotenv": "^8.2.0",
        "ejs": "^3.0.1",
        "express-session": "^1.17.0",
        "fluent-ffmpeg": "^2.1.2",
        "http-errors": "^1.7.3",
        "moment": "^2.24.0",
        "morgan": "^1.9.1",
        "multer": "^1.4.2",
        "mysql2": "^2.0.1",
        "node-media-server": "^2.1.4",
        "node-rsa": "^1.0.7",
        "reflect-metadata": "^0.1.13",
